  "scripts": {
    "dev": "npm-run-all --parallel dev:serve dev:secure",
    "start": "next dev",
    "dev:mock": "npm-run-all --parallel dev:serve dev:secure api:mock",
    "dev:serve": "next dev",
    "dev:secure": "npx local-ssl-proxy --key local.dash.jakpat.web.id-key.pem --cert local.dash.jakpat.web.id.pem --source 3001 --target 3000",
    "build": "next build",
    "serve": "next start",
    "test": "jest",
    "coverage": "./utils/test-coverage.sh",
    "coverage:component": "./utils/test-coverage-component.sh",
    "test:unit": "jest --watch",
    "test:unit:coverage": "./utils/test-coverage.sh",
    "test:e2e": "playwright test",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "next lint",
    "api:prepare": "sed \"s/{{API_DOC_MOCK_URL}}/http:\\/\\/localhost:4010/\" src/backend/openapi/API/dash/openapi.tpl.yaml > src/backend/openapi/API/dash/openapi.yaml",
    "api:bundle": "redocly bundle src/backend/openapi/API/dash/openapi.yaml -o src/backend/openapi/API/dash/bundle.yaml",
    "api:bundle:json": "redocly bundle src/backend/openapi/API/dash/openapi.yaml -o src/backend/mocks/mock-api.json --ext json",
    "api:mock": "yarn api:prepare && prism mock src/backend/openapi/API/dash/openapi.yaml",
    "api:preview": "redocly preview-docs src/backend/openapi/API/dash/openapi.yaml",
    "ts": "tsc --noEmit --incremental",
    "ts:watch": "yarn ts --watch"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "next",
      "prettier",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "react",
      "@typescript-eslint"
    ],
    "env": {
      "jest": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
